<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Vendor styles -->
    <link rel="stylesheet" href="~/Assets/admin/vendors/material-design-iconic-font/css/material-design-iconic-font.min.css">
    <link rel="stylesheet" href="~/Assets/admin/vendors/animate.css/animate.min.css">
    <link rel="stylesheet" href="~/Assets/admin/vendors/jquery-scrollbar/jquery.scrollbar.css">
    <link rel="stylesheet" href="~/Assets/admin/vendors/fullcalendar/fullcalendar.min.css">
   
    <!-- App styles -->
    <link rel="stylesheet" href="~/Assets/admin/css/app.min.css">
    <script src="~/Scripts/angular.min.js"></script>
    <script src="~/Scripts/angular.js"></script>
    <script src="~/Scripts/ng-file-upload.js"></script>
    <script src="~/Scripts/ng-file-upload.min.js"></script>
    <script src="~/Scripts/controllers/Qli.js"></script>
    <script src="~/Scripts/controllers/LoginAJS.js"></script>
    <script src="~/Scripts/dirPagination.js"></script>

    <script src="~/Scripts/angular-1.3.9/angular-1.3.9/angular.js"></script>
    <script src="~/Scripts/angular-1.3.9/angular-1.3.9/angular.min.js"></script>
    <script src="~/Scripts/ui-bootstrap-tpls-0.13.4.min.js"></script>
    @*<link href="~/Assets/admin/css/bootstrap.min.css" rel="stylesheet" />*@
    <style>
        * {
            font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif;
        }
    </style>
</head>
<body data-ma-theme="blue-grey" ng-app="MyApp">

    <main class="main">
        <div class="page-loader">
            <div class="page-loader__spinner">
                <svg viewBox="25 25 50 50">
                    <circle cx="50" cy="50" r="20" fill="none" stroke-width="2" stroke-miterlimit="10" />
                </svg>
            </div>
        </div>
        @RenderPage("_Header.cshtml")
        @RenderPage("_Sidebar.cshtml")
        <section class="content">
            <header class="content__title">
                <h1>DATA TABLES</h1>

                <div class="actions">
                    <a href="" class="actions__item zmdi zmdi-trending-up"></a>
                    <a href="" class="actions__item zmdi zmdi-check-all"></a>

                    <div class="dropdown actions__item">
                        <i data-toggle="dropdown" class="zmdi zmdi-more-vert"></i>
                        <div class="dropdown-menu dropdown-menu-right">
                            <a href="" class="dropdown-item">Refresh</a>
                            <a href="" class="dropdown-item">Manage Widgets</a>
                            <a href="" class="dropdown-item">Settings</a>
                        </div>
                    </div>
                </div>
            </header>
            @RenderBody()
            @RenderPage("_Footer.cshtml")
        </section>
    </main>

    <script src="~/Assets/admin/vendors/jquery/jquery.min.js"></script>
    <script src="~/Assets/admin/vendors/popper.js/popper.min.js"></script>
    <script src="~/Assets/admin/vendors/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/Assets/admin/vendors/jquery-scrollbar/jquery.scrollbar.min.js"></script>
    <script src="~/Assets/admin/vendors/jquery-scrollLock/jquery-scrollLock.min.js"></script>

    <script src="~/Assets/admin/vendors/flot/jquery.flot.js"></script>
    <script src="~/Assets/admin/vendors/flot/jquery.flot.resize.js"></script>
    <script src="~/Assets/admin/vendors/flot.curvedlines/curvedLines.js"></script>
    <script src="~/Assets/admin/vendors/jqvmap/jquery.vmap.min.js"></script>
    <script src="~/Assets/admin/vendors/jqvmap/maps/jquery.vmap.world.js"></script>
    <script src="~/Assets/admin/vendors/easy-pie-chart/jquery.easypiechart.min.js"></script>
    <script src="~/Assets/admin/vendors/salvattore/salvattore.min.js"></script>
    <script src="~/Assets/admin/vendors/sparkline/jquery.sparkline.min.js"></script>
    <script src="~/Assets/admin/vendors/moment/moment.min.js"></script>
    <script src="~/Assets/admin/vendors/fullcalendar/fullcalendar.min.js"></script>
    <!-- Vendors: Data tables -->
    <script src="~/Assets/admin/vendors/datatables/jquery.dataTables.min.js"></script>
    <script src="~/Assets/admin/vendors/datatables-buttons/dataTables.buttons.min.js"></script>
    <script src="~/Assets/admin/vendors/datatables-buttons/buttons.print.min.js"></script>
    <script src="~/Assets/admin/vendors/jszip/jszip.min.js"></script>
    <script src="~/Assets/admin/vendors/datatables-buttons/buttons.html5.min.js"></script>



    <script src="~/Assets/admin/js/app.min.js"></script>
    <script src="~/Assets/admin/vendors/jquery-mask-plugin/jquery.mask.min.js"></script>
    <script src="~/Assets/admin/vendors/select2/js/select2.full.min.js"></script>
    <script src="~/Assets/admin/vendors/dropzone/dropzone.min.js"></script>
    <script src="~/Assets/admin/vendors/moment/moment.min.js"></script>
    <script src="~/Assets/admin/vendors/nouislider/nouislider.min.js"></script>
    <script src="~/Assets/admin/vendors/trumbowyg/trumbowyg.min.js"></script>
    <script src="~/Assets/admin/vendors/flatpickr/flatpickr.min.js"></script>
    <script src="~/Assets/admin/vendors/rateyo/jquery.rateyo.min.js"></script>
    <script src="~/Assets/admin/vendors/jquery-text-counter/textcounter.min.js"></script>
    <script src="~/Assets/admin/vendors/autosize/autosize.min.js"></script>
    <script src="~/Assets/admin/vendors/bootstrap-colorpicker/js/bootstrap-colorpicker.min.js"></script>
    <script src="~/Assets/admin/vendors/bootstrap-notify/bootstrap-notify.min.js"></script>
    <script src="~/Assets/admin/vendors/sweetalert2/sweetalert2.min.js"></script>
    <!-- App functions and actions -->
    <script src="~/Assets/admin/js/app.min.js"></script>
    <script>$.fn.dataTableExt.sErrMode = 'throw';</script>
    @RenderSection("script", required: false)
</body>
</html>